[editor]

movie_writer/movie_file="S:/GameDevelopment/02_GamesInProgress/GraveyardShift/graveyardshift/99_Test_Videos/Test_Footage.avi"

[editor_plugins]

enabled=PackedStringArray("res://addons/gltf2meshlib/plugin.cfg")

